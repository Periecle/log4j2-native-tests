<Configuration status="INFO">
	<Appenders>
		<Console name="ConsoleAppender" target="SYSTEM_OUT">
			<PatternLayout pattern="%d{YYYY-MM-dd HH:mm:ss} [%t] %-5p %c - %m%n" />
		</Console>
	</Appenders>

	<Loggers>
		<Root level="INFO">
			<AppenderRef ref="ConsoleAppender"/>
		</Root>
		<Logger name="org.graalvm.log4j2" level="DEBUG" />
	</Loggers>
</Configuration>